<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="270.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxprt.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TableView fx:id="mainTable" editable="true" prefHeight="200.0" prefWidth="200.0">
       <columns>
           <TableColumn prefWidth="75.0" text="id" />
           <TableColumn prefWidth="75.0" text="name" />
           <TableColumn prefWidth="75.0" text="genre" />
           <TableColumn prefWidth="75.0" text="duration" />
           <TableColumn prefWidth="75.0" text="year" />
       </columns>
   </TableView>

    <Label fx:id="welcomeText" />
</VBox>
